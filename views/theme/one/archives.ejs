<div id="main" class="container page">
    <div class="content">
        <article class="post block">
            <h1 class="title">文章存档</h1>
            <div class="entry archives">
                <%archives.forEach(function(archive){%>
                <h2><%=archive.archiveName%></h2>
                <ul>
                    <%archive.posts.forEach(function(item){%>
                    <li>
                        <time datetime="<%=item.created%>"><%=locals.moment(item.created).format("MM/DD")%></time>
                        <a href="/post/<%=item.slug%>"><%=item.title%></a>
                    </li>
                    <%})%>
                </ul>
                <%})%>
            </div>
        </article>
    </div>
</div>